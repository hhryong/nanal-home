            <artifactId>mysql-connector-java</artifactId>
            <version>5.1.49</version>
        </dependency>
        <dependency>
            <groupId>cubrid</groupId>
            <artifactId>cubrid-jdbc</artifactId>
            <version>11.0.2.0291</version>
        </dependency>
        <dependency>
            <groupId>local</groupId>
            <artifactId>sqljdbc</artifactId>
            <version>2</version>
